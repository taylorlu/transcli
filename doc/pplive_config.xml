<?xml version="1.0" ?> 
<transcode>
  <mbrset>
    <level name="FullHD" prefix="2100k" brmin="2000k" brmax="2400k" widthmin="1280" widthmax="1920"/>
    <level name="BD" prefix="1200k" brmin="1000k" brmax="1300k" widthmin="960" widthmax="1280"/>
    <level name="HD" prefix="650k" brmin="500k" brmax="700k" widthmin="640" widthmax="960"/>
    <level name="SD" prefix="400k" brmin="300k" brmax="450k" widthmin="320" widthmax="640"/>
    <level name="m300" prefix="mobile" brmin="250k" brmax="320k" widthmin="320" widthmax="640"/>
    <level name="m156" prefix="156k" brmin="140k" brmax="170k" widthmin="240" widthmax="320"/>
    <level name="m50" prefix="50k" brmin="40k" brmax="60k" widthmin="120" widthmax="176"/>
  </mbrset>
  <source>
    <type>0</type>  <!--0:Music 1:Movie 2:Episode 3:Anime 4:TVShow 5:Sport 6:Game-->
    <demuxer>0</demuxer>  <!--0:default, 1:lavf, change demuxer of ts file-->
	<!--track config:single(0)/original(1)/dual(2)-->
    <audio stream="0" channel="both" decoder="auto" track="0"/> <!--"both", "left", "right", "mix", "original"-->
    <video stream="0" rectangle="{0,0,480,360}" crop="auto" threshold="26" decoder="auto"/> <!--crop:disable(0)/manual(1)/auto(4)/calc(2)/expand(3)-->
    <clipmode>0</clipmode>  <!--0:frame accurate(transcoding), 1:I-frame accurate(may be 5 secs error)-->
    <clip start="00:00:02.0" end="00:01:10.0"/>   <!--First clip-->
    <clip start="00:01:20.0" end="00:01:40.0"/>   <!--Second clip, can be more clips-->
  </source>

  <delogo>
    <pos position="{135,50,223,120}" start="00:00:00.0" end="23:00:00" />
    <pos position="{235,100,323,180}" start="00:00:00.0" end="23:00:00" />
  </delogo>
  
  <target>
    <cores>0</cores> <!--Transcli running on some cores, default is 0, running on all cores.-->
    <audio disable="false">
      <codec format="aac" bitrate="48k" samplerate="24000" timeshift="1.5s" brdown="true"/>
      <volgain mode="auto" value="0.0"/> <!--mode:auto/manual,default is auto. value range:-200~60-->
    </audio>
    <video disable="false">
      <lowerbitrate>1</lowerbitrate> <!--0:disable decreasing bitrate, 1:enable(default)-->
      <videoenhance>1</videoenhance> <!--0:disable video enhance, 1:enable(default)-->
      <rcmode>4</rcmode>  <!--1:ABR, 2:CRF, 3:CBR, 4:2-pass-->
      <ref>4</ref>  <!--Reframes, default is 4-->
      <codec format="h264" bitrate="380k" rectangle="{0,0,176,144}" position="{10,10,166,134}" encode="true"/> <!--encode:true(do video encoding), false(not do video encoding)-->
      <fps>0</fps>
      <dar>0.0</dar>  <!--Float value, Should be parsed to darNum and darDen-->
      <keyint>5</keyint> <!--Key frame interval. If keyint < 10, it's senconds else it's frame num.-->
	  <bframes>4</bframes> <!--B frames num. Range:[0,16], default is 4.-->
    </video>
    <subtitle id="-1" timeshift="0s" /> <!--id:-1 auto select embed subtitle, -2 disable embed subtitle-->
    <logo>
      <graph file="{D:\logoTest\community_1中6.png}" interval="1000ms" /> 
      <show>
        <relativepos corner="2" offsetx="10" offsety="10" width="0" height="0" speed="0" wait="0" route="x,c"/>  <!--Relative mode(LU:1 RU:2 RD:3 LD:4)-->
        <pos position="{5,5,60,60}" start="00:00:00" end="00:00:20" /> 
        <pos position="{585,5,640,60}" start="00:00:20" end="00:00:50" /> 
        <pos position="{5,5,60,60}" start="00:00:50" end="00:02:00" /> 
      </show>
    </logo>
    <filter>
      <color red="+10%" bright="+50%" /> 
      <noise depth="12" mask="8x8" />
      <denoise mode="0" luma="4" chroma="3" temporal="6"/>
      <deinterlace mode="0" algorithm="0"/> <!--mode:0(auto), 1(enable), 2(disable)-->
    </filter>
    <mux format="flv" disable="false"/>  <!--Muxer format:flv/mp4/ts/hls disable:enable or disable muxing-->
    <!--type:average/none, size:segment size(sec), postfix:index/time/.mp4.%d, lastsegpercent:20%, threshold:duration>=1800 segment-->
    <segment type="average" unit="time" size="40" postfix="index" threshold="1800" lastsegpercent="20"/>
    <!--Static image:image and druation. Dynamic image tail:folder-->
    <imagetail image="" folder="" duration="2" cropmode="0"/>
    <!--type:.m3u8/.m3u/.pls/.wpl/.csv, name:playlist name(when empty use source file name), postfix:name postfix, live:if live stream-->
    <playlist type=".m3u8" name="" postfix=".mp4" live="false"/>
	<!--Apple HLS format. dur:segment duration(s), listsize:how many segments one list contain,startindex:segment file start number -->
	<hls dur="5" listsize="100000" postfix=".mp4" startindex="1"/>
    <!--start:start time, end:end time, interval:frequency, row:tile row, col:tile column, type:1(png)/2(jpg)/3(bmp)/4(gif), count:thumbnail count, quality:Jpeg quality(max:100), stitch:tile thumbnail to big image, pack:pack images to ipk file-->
    <thumbnail start="5" end="0" interval="6" row="10" col="10" width="180" height="0" type="2" count="0" quality="85" stitch="true" pack="true" postfix="_shot"/> <!--postfix:postfix of thumbnail's file name-->
    <!--ignoreErrIdx: 0(no ignore), 1(ignore 32), 2(ignore33), 3(ignore both)-->
    <errorignore>0</errorignore>
    
    <additon>
      <x264 option="-level=4.1,-fourcc=H.264,-profile=Baseline/Main/High">
        <zone start="00:00:02.0" end="00:01:10.0" ratio="0.6"/>    <!--Lower bitrate of the zone-->
        <zone start="00:01:20.0" end="00:01:40.0" ratio="0.6"/>    <!--Lower bitrate of the zone-->
      </x264>
      <mp4 option="-brand=3grp/3gp6/3gp4,-version=512,-hint=true,-sbr=0/1/2" /> <!--sbr:0-default, 1-sbr, 2-sbrx-->
	  <x265 option="-preset=fast,-me=3,-subme=3,-sao=true,-amp=true,-rect=true,-badapt=1,-wpp=true,-ctu=64,-F=5,-rdLevel=1,-refreshType=2,-loopFilter=true,-bpyramid=true," />
    </additon>
  </target>
</transcode>
